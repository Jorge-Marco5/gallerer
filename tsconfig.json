{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "darkMode": "class",
  "content": [
    "./src/**/*.{astro,js,jsx,ts,tsx}",
  ],
  "theme": {
    "extend": {}
  },
  "plugins": [],
}